{
  "name": "@tachui/intro-app",
  "version": "0.1.0",
  "description": "tachUI Introduction - Marketing landing page recreated with tachUI framework",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite* && vite",
    "build": "vite build",
    "build:clean": "rm -rf node_modules/.vite* && vite build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite*"
  },
  "dependencies": {
    "@tachui/core": "workspace:*",
    "@tachui/devtools": "workspace:*",
    "@tachui/effects": "workspace:*",
    "@tachui/flow-control": "workspace:*",
    "@tachui/modifiers": "workspace:*",
    "@tachui/responsive": "workspace:*",
    "@tachui/symbols": "workspace:*",
    "@tachui/primitives": "workspace:*",
    "@tachui/registry": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.8.0",
    "vite": "^7.1.1",
    "vite-bundle-analyzer": "^0.11.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "keywords": [
    "tachui",
    "intro",
    "marketing",
    "landing-page",
    "swiftui",
    "reactive"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "homepage": "https://tachui.dev"
}

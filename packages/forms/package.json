{
  "name": "@tachui/forms",
  "version": "0.1.0",
  "description": "Form components and validation system for tachUI framework",
  "homepage": "https://tachui.dev/",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "type-check": "tsc --noEmit",
    "lint": "oxlint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "tachui",
    "forms",
    "validation",
    "ui",
    "components",
    "plugin"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tach-UI/tachUI",
    "directory": "packages/forms"
  },
  "peerDependencies": {
    "@tachui/core": "^0.1.0"
  },
  "devDependencies": {
    "@tachui/core": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js"
    },
    "./state": {
      "types": "./dist/state/index.d.ts",
      "import": "./dist/state/index.js"
    }
  },
  "sideEffects": false
}

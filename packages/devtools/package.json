{
  "name": "@tachui/devtools",
  "version": "0.8.1-alpha",
  "description": "Development & debugging tools for tachUI framework",
  "homepage": "https://tachui.dev/",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "production": {
        "types": "./dist/index.prod.d.ts",
        "import": "./dist/index.prod.js"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./inspector": {
      "types": "./dist/inspector/index.d.ts",
      "import": "./dist/inspector/index.js"
    },
    "./profiler": {
      "types": "./dist/profiler/index.d.ts",
      "import": "./dist/profiler/index.js"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "import": "./dist/debug/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "valid": "clear && vitest run && pnpm type-check && pnpm lint && pnpm build",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "oxlint src"
  },
  "dependencies": {
    "@tachui/core": "workspace:*",
    "@tachui/primitives": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jsdom": "^21.1.6",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^23.2.0",
    "typescript": "^5.8.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "tachui",
    "devtools",
    "debugging",
    "development",
    "inspector",
    "profiler",
    "testing"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tach-UI/tachUI",
    "directory": "packages/devtools"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": false
}

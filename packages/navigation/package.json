{
  "name": "@tachui/navigation",
  "version": "0.8.0-alpha",
  "description": "Navigation components for tachUI - NavigationView, TabView, and routing utilities",
  "homepage": "https://tachui.dev/",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:dev": "rm -rf dist && vite build --mode development && tsc --project tsconfig.build.json",
    "build:prod": "rm -rf dist && vite build --mode production && tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "valid": "clear && vitest run && pnpm type-check && pnpm lint && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "bench": "vitest bench",
    "benchmark": "vitest bench --config vitest.benchmark.config.ts benchmarks/navigation-performance.bench.ts",
    "benchmark:quick": "vitest bench --config vitest.benchmark.config.ts benchmarks/quick.bench.ts",
    "benchmark:bundle": "vitest bench --config vitest.benchmark.config.ts benchmarks/bundle-size.bench.ts",
    "benchmark:browser": "playwright test benchmarks/browser.spec.ts",
    "benchmark:all": "pnpm benchmark && pnpm benchmark:bundle && pnpm benchmark:browser",
    "lint": "oxlint src"
  },
  "dependencies": {},
  "peerDependencies": {
    "@tachui/core": "^0.8.0-alpha"
  },
  "devDependencies": {
    "@tachui/core": "workspace:*",
    "@tachui/primitives": "workspace:*",
    "@types/node": "^20.0.0",
    "@playwright/test": "^1.40.0",
    "typescript": "^5.8.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "tachui",
    "navigation",
    "swiftui",
    "web-framework",
    "typescript",
    "reactive"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tach-UI/tachUI",
    "directory": "packages/navigation"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": false
}

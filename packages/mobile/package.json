{
  "name": "@tachui/mobile",
  "version": "0.1.0",
  "description": "Mobile UI components for TachUI - Core modal components (ActionSheet, Alert) with roadmap for advanced mobile features",
  "homepage": "https://tachui.dev/",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && vite build",
    "build:types": "echo 'TypeScript types provided by @tachui/core'",
    "dev": "vite build --watch",
    "lint": "oxlint src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tachui/core": "^0.1.0"
  },
  "devDependencies": {
    "@tachui/core": "workspace:*",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.2.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ui",
    "mobile",
    "actionsheet",
    "alert",
    "modal",
    "tachui",
    "plugin"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tach-UI/tachUI",
    "directory": "packages/mobile"
  }
}

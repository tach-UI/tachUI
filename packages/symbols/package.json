{
  "name": "@tachui/symbols",
  "version": "0.7.1-alpha",
  "description": "SwiftUI-inspired symbol/icon system for tachUI framework",
  "homepage": "https://tachui.dev/",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "valid": "clear && vitest run && tsc --project tsconfig.type-check.json && pnpm lint && pnpm build",
    "test": "vitest run",
    "test:ci": "vitest run 2>/dev/null",
    "test:quiet": "vitest run 2>/dev/null",
    "type-check": "tsc --project tsconfig.type-check.json",
    "lint": "oxlint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "tachui",
    "symbols",
    "icons",
    "lucide",
    "ui",
    "components",
    "plugin"
  ],
  "author": "tachUI Team",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tach-UI/tachUI",
    "directory": "packages/symbols"
  },
  "peerDependencies": {
    "@tachui/core": "^0.1.0"
  },
  "dependencies": {
    "@tachui/modifiers": "workspace:*",
    "lucide": "^0.447.0"
  },
  "devDependencies": {
    "@tachui/core": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.8.0",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./icon-sets": {
      "types": "./dist/icon-sets/index.d.ts",
      "import": "./dist/icon-sets/index.js"
    },
    "./modifiers": {
      "types": "./dist/modifiers/index.d.ts",
      "import": "./dist/modifiers/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "sideEffects": [
    "./dist/index.js",
    "./src/index.ts"
  ],
  "bugs": "https://github.com/tach-UI/tachUI/issues",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

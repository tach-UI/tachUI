{
  "entryPoints": [
    "./packages/core/src/index.ts",
    "./packages/cli/src/index.ts",
    "./packages/primitives/src/index.ts",
    "./packages/modifiers/src/index.ts",
    "./packages/effects/src/index.ts",
    "./packages/flow-control/src/index.ts",
    "./packages/forms/src/index.ts",
    "./packages/navigation/src/index.ts",
    "./packages/symbols/src/index.ts",
    "./packages/data/src/index.ts",
    "./packages/grid/src/index.ts",
    "./packages/mobile/src/index.ts",
    "./packages/responsive/src/index.ts",
    "./packages/viewport/src/index.ts",
    "./packages/devtools/src/index.ts"
  ],
  "out": "./docs/api",
  "name": "tachUI API Documentation",
  "includeVersion": true,
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Reactive System",
    "Core Components",
    "Primitives",
    "Modifiers",
    "Effects",
    "Forms",
    "Navigation",
    "Data",
    "Mobile",
    "Symbols",
    "DevTools",
    "CLI Tools",
    "*"
  ],
  "groupOrder": ["Functions", "Classes", "Interfaces", "Type Aliases", "*"],
  "sort": ["source-order"],
  "exclude": ["**/*.test.ts", "**/*.bench.ts", "**/node_modules/**"],
  "validation": {
    "notExported": true,
    "invalidLink": true
  },
  "treatWarningsAsErrors": false,
  "skipErrorChecking": false,
  "plugin": ["typedoc-plugin-markdown"],
  "tsconfig": "./tsconfig.typedoc.json"
}
